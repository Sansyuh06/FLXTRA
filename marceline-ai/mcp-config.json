{
    "mcpServers": {
        "filesystem": {
            "command": "npx",
            "args": [
                "-y",
                "@modelcontextprotocol/server-filesystem",
                "D:/fyeshi/project/OS"
            ]
        },
        "brave-search": {
            "command": "npx",
            "args": [
                "-y",
                "@anthropic-ai/mcp-server-brave-search"
            ],
            "env": {
                "BRAVE_API_KEY": "YOUR_BRAVE_API_KEY_HERE"
            }
        },
        "memory": {
            "command": "npx",
            "args": [
                "-y",
                "@anthropic-ai/mcp-server-memory"
            ]
        },
        "fetch": {
            "command": "npx",
            "args": [
                "-y",
                "@anthropic-ai/mcp-server-fetch"
            ]
        }
    }
}